/Questionnaires:
  type: {
    fhirResourceType: {
      postExample: !include examples/requests/post_questionnaire.json,
      getExample:  !include examples/responses/get_questionnaire.json,
    }
  }
  get:
    is: [ questionnaireSearch ]
  put:
    is: [ questionnaireSearch ]
  delete:
    is: [ questionnaireSearch ]
  /_history:
    type: { fhirHistory: { bundleName: QuestionnaireBundle } }
    is: [ questionnaireSearch ]
  /{id}:
    type: {
      fhirResource: {
        getSingleExample: !include examples/responses/get_questionnaire_id.json,
        putExample:       !include examples/requests/put_questionnaire_id.json
      }
    }
    /_history:
      type: { fhirHistory: { bundleName: QuestionnaireBundle } }
      /{vid}:
        type: { fhirVersionedResource: { resourceName: Questionnaire } }
  /_search:
    type: { fhirSearch: { bundleName: QuestionnaireBundle } }
    get:
      is: [ questionnaireSearch ]
    post:
      is: [ questionnairePOSTSearch ]

/QuestionnaireResponses:
  type: {
    fhirResourceType: {
      postExample: !include examples/requests/post_questionnaireResponse.json,
      getExample:  !include examples/responses/get_questionnaireResponse.json,
    }
  }
  get:
    is: [ questionnaireResponseSearch ]
  put:
    is: [ questionnaireResponseSearch ]
  delete:
    is: [ questionnaireResponseSearch ]
  /_history:
    type: { fhirHistory: { bundleName: QuestionnaireResponseBundle } }
    is: [ questionnaireResponseSearch ]
  /{id}:
    type: {
      fhirResource: {
        getSingleExample: !include examples/responses/get_questionnaireResponse_id.json,
        putExample:       !include examples/requests/put_questionnaireResponse_id.json
      }
    }
    /_history:
      type: { fhirHistory: { bundleName: QuestionnaireResponseBundle } }
      /{vid}:
        type: { fhirVersionedResource: { resourceName: QuestionnaireResponse } }
  /_search:
    type: { fhirSearch: { bundleName: QuestionnaireResponseBundle } }
    get:
      is: [ questionnaireResponseSearch ]
    post:
      is: [ questionnaireResponsePOSTSearch ]

