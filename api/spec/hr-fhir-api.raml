  questionnaireResponseSearch:
    queryParameters: !include queryParams/resources/questionnaireResponseParams.yaml
  questionnaireSearch:
    queryParameters: !include queryParams/resources/questionnaireParams.yaml
  scheduleSearch:
    queryParameters: !include queryParams/resources/scheduleParams.yaml
  slotSearch:
    queryParameters: !include queryParams/resources/slotParams.yaml

