      application/json:
        properties: !include queryParams/resources/slotParams.yaml

securitySchemes:
  oauth_1_0:
    type: x-oauth-1-0
    description: |
      A custom security scheme for authenticating requests.
    describedBy:
      headers:
        x-oauth-consumer-key:
          type: string
          description: |
            OAuth consumer key
        x-oauth-consumer-secret:
          type: string
          description: |
            OAuth consumer secret
        x-oauth-token:
          type: string
          description: |
            OAuth token
        x-oauth-token-secret:
          type: string
          description: |
            OAuth token secret
      responses:
        401:
          description: |
            Invalid header(s)
        403:  # implicit unauthorized

securedBy: oauth_1_0

